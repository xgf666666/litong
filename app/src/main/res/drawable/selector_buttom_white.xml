<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:width="1px" android:color="@color/button_line_color"/>
            <solid android:color="@color/button_white_color_pressed" />
            <!--<gradient android:centerColor="#DDF77CC3" android:endColor="#DDFFB999" android:startColor="#DD76B7FD" />-->
            <corners android:radius="2dp" />
        </shape>
    </item>
    <item>
        <shape>
            <padding android:bottom="2dp" android:left="2dp" android:right="2dp" android:top="2dp"/>
            <stroke android:width="1px" android:color="@color/button_line_color"/>
            <!--<gradient android:centerColor="#F77CC3" android:endColor="#FFB999" android:startColor="#76B7FD" />-->
            <solid android:color="@color/button_white_color_normal" />
            <corners android:radius="2dp" />
        </shape>
    </item>
</selector>